<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel = "tests" name="Suite">
	<listeners>
		<listener class-name="framework.TestComponents.Listeners"/>
	</listeners>
  <test thread-count="5" name="Actual Test">
    <classes>
      <class name="framework.tests.ActualTest"/>
    </classes>
  </test> <!-- Test -->

  <test thread-count="5" name="Error Validations">
    <classes>
      <class name="framework.tests.ErrorValidations"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
